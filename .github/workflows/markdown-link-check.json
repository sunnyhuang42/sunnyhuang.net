{
  "ignorePatterns": [
    {
      "reason": "Testing Feishu URLs results in too many redirections",
      "pattern": "https://.*.feishu.cn"
    },
    {
      "reason": "LinkedIn does not allow scanning",
      "pattern": "https://www.linkedin.com"
    },
    {
      "reason": "Scanning igetget.com results in socket timeout",
      "pattern": "https://m.igetget.com"
    },
    {
      "reason": "Scanning getit01.com results in socket timeout",
      "pattern": "https://www.getit01.com"
    },
    {
      "reason": "Testing Douban URLs results in socket timeout",
      "pattern": "https://.*.douban.com"
    },
    {
      "reason": "Testing Weibo IR URLs results in socket timeout",
      "pattern": "http://ir.weibo.com/"
    },
    {
      "reason": "",
      "pattern": "https://img1.iyiou.com"
    },
    {
      "reason": "Scanning apple store URLs results in socket timeout",
      "pattern": "https://itunes.apple.com"
    },
    {
      "reason": "Website no longer exists",
      "pattern": "http://hicape.com",
      "TODO": "Need to update or remove"
    },
    {
      "reason": "CDN",
      "pattern": "https://cdn.sunnyhuang.net/"
    },
    {
      "reason": "",
      "pattern": "https://pic1cdn.luojilab.com/"
    },
    {
      "reason": "",
      "pattern": "https://twitter.com/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://sunnyhuang.net/"
    }
  ],
  "timeout": "10s",
  "retryCount": 2,
  "retryOn429": false,
  "aliveStatusCodes": [200, 206, 403, 429, 500, 503]
}
